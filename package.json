{
  "name": "grpc-web-devtools",
  "version": "1.0.0",
  "main": "index.js",
  "author": "IvanRodriCalleja <ivanrodricalleja@gmail.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prepare": "panda codegen",
    "dev:chrome": "concurrently \"yarn dev:example\" \"cross-env BROWSER=chrome vite build --watch\" \"yarn launch:chrome\"",
    "build:chrome:local": "cross-env BROWSER=chrome vite build",
    "build:chrome:pro": "cross-env BROWSER=chrome vite build",
    "prettier": "prettier --write '**/*.{js,jsx,css,html}'",
    "launch:chrome": "node ./chrome/dev.js",
    "tsc": "tsc",
    "lint": "eslint src --ext .ts,.tsx --quiet --cache",
    "test": "jest -c jest.config.cjs",
    "test:watch": "jest -c jest.config.cjs --watch",
    "dev:example": "yarn --cwd \"example\" dev",
    "vite:dev": "chokidar \"**/*.ts\" -i \"./chrome/**\" -i \"./src/auto-imports.d.ts\" -i \"./example/**\" -i \"./node_modules/**\" --initial -c \"cross-env BROWSER=chrome vite build\""
  },
  "dependencies": {
    "@pandacss/dev": "^0.9.0",
    "google-protobuf": "^3.21.2",
    "grpc-web": "^1.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "webextension-polyfill": "^0.10.0"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "2.0.0-beta.18",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/chrome": "0.0.242",
    "@types/google-protobuf": "^3.15.6",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@types/webextension-polyfill": "^0.10.1",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "child-process-promise": "^2.2.1",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.3.1",
    "prettier": "^3.0.0",
    "typescript": "5.1.6",
    "unplugin-auto-import": "^0.12.1",
    "vite": "4.4.7",
    "vite-plugin-environment": "1.1.3",
    "web-ext": "7.6.2"
  }
}
