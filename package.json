{
  "name": "grpc-web-devtools",
  "version": "1.0.0",
  "main": "index.js",
  "author": "IvanRodriCalleja <ivanrodricalleja@gmail.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev:chrome": "concurrently \"yarn launch:chrome\" \"vite build --watch --outDir=./chrome/build\"",
    "build:chrome:local": "vite build --outDir=./chrome/build",
    "build:chrome:dev": "vite build --outDir=./chrome/build --mode=dev",
    "build:chrome:pro": "vite build --outDir=./chrome/build --mode=pro",
    "prettier": "prettier --write '**/*.{js,jsx,css,html}'",
    "launch:chrome": "node ./chrome/dev.js",
    "tsc": "tsc",
    "lint": "eslint src --ext .ts,.tsx --quiet --cache",
    "test": "jest -c jest.config.cjs",
    "test:watch": "jest -c jest.config.cjs --watch"
  },
  "dependencies": {
    "webextension-polyfill": "^0.10.0"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "2.0.0-beta.18",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/chrome": "0.0.242",
    "@types/webextension-polyfill": "^0.10.1",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "child-process-promise": "^2.2.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.3.1",
    "prettier": "^3.0.0",
    "typescript": "5.1.6",
    "unplugin-auto-import": "^0.12.1",
    "vite": "4.4.7",
    "vite-plugin-environment": "1.1.3",
    "web-ext": "7.6.2"
  }
}
